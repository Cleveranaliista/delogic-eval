<project xmlns="http://maven.apache.org/POM/4.0.0"         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0                             http://maven.apache.org/xsd/maven-4.0.0.xsd">    <modelVersion>4.0.0</modelVersion>    <groupId>com.example</groupId>    <artifactId>delogic-eval</artifactId>    <version>1.0.0</version>    <name>delogic-eval</name>    <description>Projeto de lógica promocional</description>    <parent>        <groupId>org.springframework.boot</groupId>        <artifactId>spring-boot-starter-parent</artifactId>        <version>3.1.5</version>        <relativePath/>    </parent>    <properties>        <java.version>17</java.version>        <!-- Definindo a codificação para UTF-8 -->        <encoding>UTF-8</encoding>    </properties>    <dependencies>        <!-- Starter Web -->        <dependency>            <groupId>org.springframework.boot</groupId>            <artifactId>spring-boot-starter-web</artifactId>        </dependency>        <!-- JPA/Hibernate -->        <dependency>            <groupId>org.springframework.boot</groupId>            <artifactId>spring-boot-starter-data-jpa</artifactId>        </dependency>        <!-- MySQL Connector -->        <dependency>            <groupId>mysql</groupId>            <artifactId>mysql-connector-java</artifactId>            <version>8.0.32</version>            <scope>runtime</scope>        </dependency>        <!-- Flyway Core -->        <dependency>            <groupId>org.flywaydb</groupId>            <artifactId>flyway-mysql</artifactId>            <version>9.22.0</version>        </dependency>        <dependency>            <groupId>org.flywaydb</groupId>            <artifactId>flyway-core</artifactId>            <version>9.22.0</version>        </dependency>        <!-- Gson -->        <dependency>            <groupId>com.google.code.gson</groupId>            <artifactId>gson</artifactId>            <version>2.9.0</version>        </dependency>        <!-- Testes -->        <dependency>            <groupId>org.springframework.boot</groupId>            <artifactId>spring-boot-starter-test</artifactId>            <scope>test</scope>        </dependency>        <!-- Mockito para simulações -->        <dependency>            <groupId>org.mockito</groupId>            <artifactId>mockito-core</artifactId>            <scope>test</scope>        </dependency>        <!-- Swagger e OpenAPI -->        <dependency>            <groupId>org.springdoc</groupId>            <artifactId>springdoc-openapi-starter-webmvc-ui</artifactId>            <version>2.1.0</version>        </dependency>    </dependencies>    <build>        <plugins>            <plugin>                <groupId>org.springframework.boot</groupId>                <artifactId>spring-boot-maven-plugin</artifactId>                <configuration>                    <!-- Classe principal do Spring Boot -->                    <mainClass>com.example.delogic_eval.AppMain</mainClass>                </configuration>            </plugin>            <!-- Configuração do maven-resources-plugin para garantir a codificação UTF-8 -->            <plugin>                <groupId>org.apache.maven.plugins</groupId>                <artifactId>maven-resources-plugin</artifactId>                <version>3.3.1</version>                <configuration>                    <encoding>UTF-8</encoding>                </configuration>            </plugin>        </plugins>    </build>    <repositories>        <repository>            <id>central</id>            <url>https://repo.maven.apache.org/maven2</url>        </repository>    </repositories></project>